{
    "name": "tradingapp",
    "version": "1.0.0",
    "description": "",
    "main": "wwwroot/index.html",
    "author": "Bac JAE",
    "license": "ISC",
    "scripts": {
        "ngc": "ngc -p ./tsconfig-aot.json",
        "start": "concurrently \"webpack-dev-server --inline --port 3000\" \"dotnet watch run\" ",
        "webpack-dev": "set NODE_ENV=development && webpack",
        "webpack-production": "set NODE_ENV=production && webpack",
        "build-dev": "npm run webpack-dev",
        "build-production": "npm run ngc && npm run webpack-production",
        "watch-webpack-dev": "set NODE_ENV=development && webpack --watch --color",
        "watch-webpack-production": "npm run build-production --watch --color",
        "publish-for-iis": "npm run build-production && dotnet publish -c Release",
        "install": "dotnet restore",
        "clean": "npm run clean:dotnet && npm run clean:npm",
        "clean:dotnet": "npm run clean:bin && npm run clean:obj",
        "clean:bin": "del /q .\\bin\\*.* && for /d %i in (.\\bin\\*.*) do @rmdir /s /q \"%i\" ",
        "clean:obj": "del /q .\\obj\\*.* && for /d %i in (.\\obj\\*.*) do @rmdir /s /q \"%i\" ",
        "clean:npm": "del /q .\\node_modules\\*.* && for /d %i in (.\\node_modules\\*.*) do @rmdir /s /q \"%i\" "
    },
    "dependencies": {
        "@angular/common": "~4.0.1",
        "@angular/compiler": "~4.0.1",
        "@angular/compiler-cli": "~4.0.1",
        "@angular/core": "~4.0.1",
        "@angular/forms": "~4.0.1",
        "@angular/http": "~4.0.1",
        "@angular/platform-browser": "~4.0.1",
        "@angular/platform-browser-dynamic": "~4.0.1",
        "@angular/platform-server": "~4.0.1",
        "@angular/router": "~4.0.1",
        "@angular/upgrade": "~4.0.1",
        "angular-in-memory-web-api": "0.3.1",
        "bootstrap": "^3.3.7",
        "core-js": "2.4.1",
        "font-awesome": "^4.7.0",
        "ie-shim": "~0.1.0",
        "reflect-metadata": "0.1.10",
        "rxjs": "5.3.0",
        "zone.js": "0.8.5"
    },
    "devDependencies": {
        "@types/node": "7.0.12",
        "angular-router-loader": "^0.6.0",
        "angular2-template-loader": "0.6.2",
        "awesome-typescript-loader": "^3.1.2",
        "browser-sync": "^2.18.8",
        "browser-sync-webpack-plugin": "^1.1.4",
        "clean-webpack-plugin": "^0.1.16",
        "concurrently": "^3.4.0",
        "copy-webpack-plugin": "^4.0.1",
        "css-loader": "^0.28.0",
        "file-loader": "^0.11.1",
        "html-webpack-plugin": "^2.28.0",
        "jquery": "^3.2.1",
        "json-loader": "^0.5.4",
        "node-sass": "^4.5.2",
        "raw-loader": "^0.5.1",
        "rimraf": "^2.6.1",
        "sass-loader": "^6.0.3",
        "source-map-loader": "^0.2.1",
        "style-loader": "^0.16.1",
        "ts-helpers": "^1.1.2",
        "tslint": "^5.0.0",
        "tslint-loader": "^3.5.2",
        "typescript": "2.2.2",
        "url-loader": "^0.5.8",
        "webpack": "^2.3.3",
        "webpack-dev-server": "2.4.2",
        "webpack-livereload-plugin": "^0.11.0"
    },
    "-vs-binding": {
        "ProjectOpened": [
            "watch-webpack-dev"
        ]
    }
}
